import RISCV_Definitions;

/*
 * Input and output ports
 */
port in reg_index rd_i, out rd_o, in data_t value_i, out value_o;

/**
 * Task
 */
combinational void main() {
	rd_o.write(rd_i.read);
	value_o.write(value_i.read);
}
